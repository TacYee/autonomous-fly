{

    "attitude":{
        "name": "attitude",
        "type": "CF",
        "period": 10,
        "variables": ["stabilizer.roll",
                      "stabilizer.pitch",
                      "stabilizer.yaw"],
        "headers": ["roll", "pitch", "yaw"]
    },

    "motor":{
        "name": "motor",
        "type": "CF",
        "period": 10,
        "variables": ["motor.m1",
                      "motor.m2",
                      "motor.m3",
                      "motor.m4"
                      ],
        "headers": ["motor1", "motor2", "motor3", "motor4"]
    },

    "state":{
        "name": "state",
        "type": "CF",
        "period": 10,
        "variables": ["stateEstimate.x",
                      "stateEstimate.y",
                      "stateEstimate.z",
                      "stateEstimate.vx",
                      "stateEstimate.vy",
                      "stateEstimate.vz",
                      "stateEstimate.yaw"],
        "headers": ["stateX", "stateY", "stateZ", "stateVX", "stateVY", "stateVZ","stateyaw"]
    },

    "whisker":{
        "name": "Whisker",
        "type": "CF",
        "period": 10,
        "variables": ["Whisker.Barometer1",
                      "Whisker.Barometer2",
                      "Whisker.Barometer3"],
        "headers": ["whisker1", "whisker2", "whisker3"]
    },

    "flow":{
        "name": "flow",
        "type": "CF",
        "period": 10,
        "variables": ["motion.deltaX",
                      "motion.deltaY"],
        "headers": ["flowX", "flowY"]
    },
    
    "laser":{
        "name": "laser",
        "type": "CF",
        "period": 10,
        "variables": ["range.front"],
        "headers": ["laserFront"]
    },

    "kalman":{
        "name": "kalman",
        "type": "CF",
        "period": 10,
        "variables": ["kalman.stateX",
                      "kalman.stateY",
                      "kalman.stateZ",
                      "kalman.statePX",
                      "kalman.statePY",
                      "kalman.statePZ"],
        "headers": ["kalX", "kalY", "kalZ", "kalPX", "kalPY", "kalPZ"]
    }
}
